{
  "name": "perfect-schedule",
  "version": "0.0.0",
  "description": "A fullstack schedule generator based on mern-starter",
  "scripts": {
    "test": "mocha shared/tests/*.spec.js --compilers js:babel-register",
    "test:server": "cross-env NODE_ENV=test PORT=8080 MONGO_URL=mongodb://localhost:27017/mern-test mocha --compilers js:babel-register --recursive server/tests/**/*.spec.js",
    "start": "cross-env NODE_ENV=development node index.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "bs": "npm run clean && npm run build && sudo systemctl restart perfect-schedule",
    "minify": "cleancss -o static/css/app.min.css static/css/app.css",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js && npm run minify",
    "clean": "rimraf static/dist",
    "slate": "rimraf node_modules && npm install",
    "lint": "eslint client server shared"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lorix-lpan/perfect-schedule"
  },
  "bugs": {
    "url": "git+https://github.com/lorix-lpan/perfect-schedule/issues"
  },
  "homepage": "https://github.com/Hashnode/mern-starter#readme",
  "author": "Lawrence Pan <lawrencefeipan@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "ansi-to-html-umd": "^0.4.2",
    "axios": "^0.9.1",
    "babel-core": "^6.4.5",
    "babel-preset-stage-0": "^6.5.0",
    "body-parser": "^1.14.2",
    "cuid": "^1.3.8",
    "es6-promise": "^3.1.2",
    "express": "^4.13.4",
    "flexboxgrid": "^6.3.0",
    "history": "^1.17.0",
    "material-ui": "0.14.2",
    "mongodb": "^2.1.7",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-motion": "^0.4.2",
    "react-redux": "^4.1.2",
    "react-router": "^2.0.0-rc5",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "^3.1.5",
    "redux-thunk": "^1.0.3",
    "sanitize-html": "^1.11.3",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.1.0",
    "babel-register": "^6.4.3",
    "chai": "^3.5.0",
    "clean-css": "^3.4.9",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "css-modules-require-hook": "^2.1.0",
    "deep-freeze": "0.0.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-plugin-react": "^3.16.1",
    "expect": "^1.13.4",
    "expect-jsx": "^2.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "mocha": "^2.4.5",
    "pre-commit": "^1.1.2",
    "react-addons-test-utils": "^0.14.7",
    "react-transform-hmr": "^1.0.1",
    "redux-devtools": "^3.1.1",
    "redux-devtools-dock-monitor": "^1.1.0",
    "redux-devtools-log-monitor": "^1.0.4",
    "rimraf": "^2.5.1",
    "style-loader": "^0.13.0",
    "supertest": "^1.1.0",
    "webpack": "^1.12.12",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.6.4"
  },
  "engines": {
    "node": ">=4"
  }
